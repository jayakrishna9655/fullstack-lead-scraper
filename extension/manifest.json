{
  "manifest_version": 3,
  "name": "Universal Lead Extractor",
  "version": "1.0",
  "permissions": ["scripting", "activeTab", "background"],
  "host_permissions": ["http://localhost:8080/*", "<all_urls>"],

  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
